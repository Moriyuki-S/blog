		"tailwind-scrollbar": "^3.1.0",
		"tailwindcss": "^3.4.13",
		"ts-node": "^10.9.2",
		"tslib": "^2.7.0",
		"typescript": "^5.6.2",
		"vite": "^5.4.8",
		"vitest": "^2.1.1"
	},
	"type": "module",
	"dependencies": {
